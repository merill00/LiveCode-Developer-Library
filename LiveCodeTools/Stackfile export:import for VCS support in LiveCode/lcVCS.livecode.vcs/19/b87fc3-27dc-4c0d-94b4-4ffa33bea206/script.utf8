function CompatibleArrayEncode pArrayA
   return arrayEncode(pArrayA)
end CompatibleArrayEncode

function NativeToUTF8 pString
   return uniDecode(uniEncode(pString,"ANSI"),"UTF8")
end NativeToUTF8

function NativeFromUTF8 pString
   return uniDecode(uniEncode(pString,"UTF8"),"ANSI")
end NativeFromUTF8

function UnicodeToUTF8 pString
   return uniDecode(pString,"UTF8")
end UnicodeToUTF8

function UnicodeFromUTF8 pString
   return uniEncode(pString,"UTF8")
end UnicodeFromUTF8

function GetWidgetProperties pObject
   return empty
end GetWidgetProperties

command SetWidgetProperties pPropertiesA 
   return empty
end SetWidgetProperties

function IsString pString
   return pString is an ascii string
end IsString
